<nlog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<targets>
		<target name="logfile" xsi:type="File" fileName="C:\LOG\GenealogyTree.log"
				layout="${longdate}|${uppercase:${level}}|${callsite}|${threadid}|${message}"
				archiveEvery="Day"
				archiveFileName="C:\LOG\Archive\GenealogyTree.{#}.log"
				archiveDateFormat="yyyy-MM-dd"/>
	</targets>
	<rules>
		<logger name="Microsoft.*" maxLevel="Info" final="true" />
		<logger name="*" minlevel="Trace" writeTo="logfile" />
	</rules>
</nlog>
